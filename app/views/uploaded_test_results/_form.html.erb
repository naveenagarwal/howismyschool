<%= form_for(@test_results_from_file, url: uploaded_test_results_path) do |f| %>
  <div id="form_errors">
    <% render partial: "/shared/form_errors", locals: { obj: @test_results_from_file, name: "Document" } %>
  </div>

  <div class="field">
    <div class="inline-div">
      <%= f.label :name, required_span_with("Select Document (Must be in .csv format)") %>
      <%= f.file_field :name %>
    </div>

  </div>

  <div class="actions inline-div width-100-percent">
    <%= f.submit "Upload" %> / <%= link_to "Download Sample File", "/sample_downloads/test_result_sample.csv" %>
  </div>
<% end %>