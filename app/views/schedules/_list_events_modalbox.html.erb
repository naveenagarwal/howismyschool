<div class="modal fade" id="list_day_schedules" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span class="sr-only">Close</span></button>
        <h4 class="modal-title my-orange" id="myModalLabel">Events for the day <%= "#{params[:month]}/#{params[:day]}/#{params[:year]}" %></h4>
      </div>
      <div class="modal-body">
        <%= render 'list_events' %>
      </div>
      <div class="modal-footer">
        <!-- <button type="button" class="btn btn-default" data-dismiss="modal">Close</button> -->
      </div>
    </div>
  </div>
</div>