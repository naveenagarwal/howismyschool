.overflow-auto{
  overflow: auto;
}
.seamless-links {
  text-align: center;
  margin: 10px;
}
.seamless-span-row, .span-row {
  overflow: hidden;
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  white-space: nowrap;
}

.seamless-item, .item {
  float: left
}

.seamless-item > span, .item > span {
  display: inline-block;
}

.seamless-item > span > img, .item > span > img {
  overflow: hidden;
}

.clear {
  clear: both;
}

#wall {
  height: 500px;
}

.full-wall, #full-wall-copy {
  height: auto;
  white-space: nowrap;
  overflow: hidden;
  float: left;
}

.wrapper {
  width: auto;
  height: auto;
  overflow: auto;
  position: relative;
}

.scale {
  position: relative;
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);
  -webkit-transform-origin: 0 0;
  -moz-transform-origin: 0 0;
  -ms-transform-origin: 0 0;
  -o-transform-origin: 0 0;
  transform-origin: 0 0;
}

.center {
  margin: 10px 0;
  text-align: center;
}

