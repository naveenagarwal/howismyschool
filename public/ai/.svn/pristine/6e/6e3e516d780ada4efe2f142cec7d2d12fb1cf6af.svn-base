li.list-menu > ul.sub-menu{
  display: none;
  background-color: #eee;
  display: none;
  left: 201px;
  list-style-type: disc;
  position: absolute;
  top: 0;
  width: 100px;
}

li.list-menu:hover > .sub-menu{
  display: block;
  background-color: #eee;
}

.contextMenu li.list-menu:hover > a {
  background-color: #eee;
  color: #333;
}

li.list-menu:hover ul.sub-menu li:hover {
  background-color: #3399FF;
}

.lightbox {
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 100;
  background: 'grey';
  display: none;
}

#user_designs_list_container{
  height: 400px;
