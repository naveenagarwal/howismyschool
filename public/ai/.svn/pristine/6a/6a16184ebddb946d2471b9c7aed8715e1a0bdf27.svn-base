li.list-menu > ul.sub-menu{
  display: none;
  background-color: #eee;
  display: none;
  left: 201px;
  list-style-type: disc;
  position: absolute;
  top: 0;
  width: 100px;
}

li.list-menu:hover > .sub-menu{
  display: block;
  background-color: #eee;
}

.contextMenu li.list-menu:hover > a {
  background-color: #eee;
  color: #333;
}

li.list-menu:hover ul.sub-menu li:hover {
  background-color: #3399FF;
}

.lightbox {
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 100;
  background: 'grey';
  display: none;
}

#user_designs_list_container{
  height: 400px;
  overflow: auto;
  margin-top: 30px;
  background: #fff;
border-radius: 10px;
border: 10px solid #fff;
}

.bring-back-toolbar{
  margin-right: 20px;
}

.bring-back-toolbar button{
  -webkit-transform: rotate(180deg);
     -moz-transform: rotate(180deg);
      -ms-transform: rotate(180deg);
       -o-transform: rotate(180deg);
          transform: rotate(180deg);
}

.red {
  color: #CB0200;
}

.font-size-20 {
  font-size: 20px;
}


#ajax-loader {
  width: 100%;
  height: 100%;
  z-index: 99999;
  opacity: 0.3;
  background-color: grey;
  position: fixed;
  left: 0px;
  top: 0px;
  align-self: center;
  background: url('../../images/ajax-loader.gif') 50% 50% no-repeat rgb(249,249,249);
  display: none;
}
