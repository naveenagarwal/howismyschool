/* Sticky footer styles
-------------------------------------------------- */
html 			{ position: relative; min-height: 100%; }
body 			{ margin-bottom: 0px; background: url(../../images/accurate-image/brick-pattern.png); font-family: Helvetica,Arial,sans-serif; }
.footer 		{ position: absolute; bottom: 0; width: 100%;  line-height: 40px;  z-index: 2;background: url(../../images/accurate-image/header-bg.png); border-top: 2px solid #ccc; }
img				{ max-width: 100%;}
:focus			{ outline: none!important;}

.footer table 	{ margin: 0px; table-layout: fixed; width: auto;}
.footer  td 	{ vertical-align: middle!important; padding: 2px 13px!important;}


/* Fonts styles
-------------------------------------------------- */
@font-face {
    font-family: 'HelveticaNeueLTCom67MediumCondensed';
    src: url('../../fonts/helveticaneueltcom-mdcn.eot');
    src: url('../../fonts/helveticaneueltcom-mdcn.eot') format('embedded-opentype'),
         url('../../fonts/helveticaneueltcom-mdcn.woff') format('woff'),
         url('../../fonts/helveticaneueltcom-mdcn.ttf') format('truetype'),
         url('../../fonts/helveticaneueltcom-mdcn.svg#HelveticaNeueLTCom67MediumCondensed') format('svg');
}


/* custom styles
-------------------------------------------------- */
.stickheader					{ background: url(../../images/accurate-image/header-bg.png);}
.stickheader .col-xs-1			{ padding: 0px;}
.stickheader .col-xs-2			{ padding: 0px;}
.stickheader td:nth-child(2)	{ padding: 0 10px;}
.sepreater-window 				{ background:url(../../images/accurate-image/screen-seprater.png) no-repeat 0 0; background-size:100% 100%; height:5px; display:block;   }
.brick-container				{  overflow: auto; }
.logo-container					{ position: relative; height: 60px;}
.logo-container	img				{ max-width: 100%; margin-left: 30px; margin-top: 10px;}
.bottom-panel	 				{ position:absolute; bottom:45px; width:100%; z-index: 9999; }
#bottom-container 				{ background:url(../../images/accurate-image/footer-panel-bg.png);  display:none; clear:both; overflow: auto; }
#btnBottom						{ width:40px; height:40px;   border:none; z-index:9; float:right; margin-right:50px; color: #fff; font-size: 24px; line-height: 16px;
							  	  border-top-right-radius: 5px; border-top-left-radius: 5px;	 background:url(../../images/accurate-image/footer-panel-bg.png); }
.hiddenDiv 						{ background: #303030;  display: block; width: 245px;  top: 0px; bottom: 0px; left: 0px; right: 0px;  float:left;  position: absolute; top: 65px;  z-index:1;}
a.btn-leftbar					{ display: inline-block; width: 40px; float:left; height: 60px;background: #303030;color: #909090;font-size: 38px; text-align: center;line-height: 70px; }
 ul.left-tabs					{ border-bottom: none;}
.left-tabs li 					{ width: 50%; text-align:center; margin-bottom: 0px; }
.left-tabs li a					{ border-radius: 0px; background: #000; font-size: 16px;  margin-right: 0px; border:none; text-decoration: none; color: #fff;  font-family: 'HelveticaNeueLTCom67MediumCondensed';}
.left-tabs li a:hover			{ background: #000; }
.left-tabs>li.active>a 			{ background:none; color: #fff; border:none;}
.left-tabs>li.active>a:hover	{ background:none; color: #fff; border:none;}
ul.social-icon-list				{ list-style-type: none; margin: 0px; padding: 0px;}
ul.social-icon-list li 			{ display: inline-block; float: right; margin-left: 8px; }
ul.social-icon-list li a		{  font-size: 150%; color: #303030;}
ul.social-icon-list li a i:hover{ color: #cb0200;  -webkit-animation: cssAnimation 1s 16 ease;-moz-animation: cssAnimation 1s 16 ease;-o-animation: cssAnimation 1s 16 ease;}
@-webkit-keyframes cssAnimation {from { -webkit-transform: rotate(360deg)  }to { -webkit-transform: rotate(4deg)   }}

button.popup-icon				{ display: inline-block;width: 30px;height: 60px;background: #303030;color: #909090;font-size: 38px; text-align: center; border: none;
								  margin-top: 33px;float: right; border-radius: 5px;}
button:hover.popup-icon 	 	{ color: #909090; }
.product-property-cont			{ color: #fff; padding: 10px; height:140px; overflow: auto; overflow-x: hidden;}
.product-property-cont h2		{ color: #9e9e9e;  font-family: 'HelveticaNeueLTCom67MediumCondensed'; font-size: 24px; margin: 0px; padding: 0px; font-weight: normal;}
.product-property-cont p		{ font-family: helvetica; font-size: 14px;}
.product-property-cont strong	{ display: block; font-weight: normal; font-size: 16px; font-family: 'Helvetica'; color: #9e9e9e; }
a.main-left-title				{ text-decoration: none; color: #fff; font-family: 'HelveticaNeueLTCom67MediumCondensed'; font-size: 24px; padding: 15px 15px 0 15px;  display: block;}



/* accordian styles
-------------------------------------------------- */
.accordion						{ width:100%;  min-height:300px;  margin:0px auto; overflow: auto;}
.accordion-item 				{ font-size: 1em; padding: 10px; background: #fff; min-height: 80px; border-bottom:2px solid #0c0c0c; color: #000; cursor:pointer; margin: 12px 12px 0px 12px;
								  border-radius: 5px; box-shadow: inset 0px 3px 20px 0 #ccc; line-height: 55px;}
.accordion-item:first-child 	{ margin-top: 30px;}
.accordion-item.open			{ border-bottom:0px; }
.accordion-item.open .type 		{ float: right;background: url(../../images/accurate-image/arrow-up-accordian.png) center no-repeat;padding: 10px; width: 35px; height: 35px; margin-top:10px; }
.accordion-item .type 			{ float: right;background: url(../../images/accurate-image/arrow-down-accordian.png) center no-repeat;padding: 10px; width: 35px; height: 35px; margin-top:10px;}
div.data 						{ background: #f7f7f7;margin:  -3px 12px  0px 12px ;padding: 10px;  display:none; border-bottom:2px solid #0c0c0c; border-bottom-left-radius: 5px;
								  border-bottom-right-radius: 5px;}


/* navigaiton styles
-------------------------------------------------- */
.main-nav							{ font-size: 14px;  word-spacing: -0.5px; margin-right: 0px; border:none; text-decoration: none; color: #2b2b2b;  font-family: 'HelveticaNeueLTCom67MediumCondensed';}
.main-nav .col-xs-1					{ padding: 0px 2px;text-align: center;height: 60px; border-right: 1px solid #ccc;padding-top: 8px;}
.main-nav .col-xs-2					{ padding: 0px; text-align:center; line-height: 12px;margin-top: 8px; }
.main-nav li 						{ color: #303030;}
.main-nav li:hover					{ text-decoration: none; color: #cb0200;}
.stack-icon							{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 0; margin: 0px auto;}
.main-nav li:hover .stack-icon 		{ background-position: -40px 0px; }
.runing-icon						{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -30px; margin: 0px auto;}
.main-nav li:hover .runing-icon 		{ background-position: -40px -30px; }
.third-icon							{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -60px; margin: 0px auto;}
.main-nav li:hover .third-icon 		{ background-position: -40px -60px; }
.coursing-icon						{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -90px; margin: 0px auto;}
.main-nav li:hover .coursing-icon	{ background-position: -40px -90px; }
.reset-icon							{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -120px; margin: 0px auto;}
.main-nav li:hover .reset-icon		{ background-position: -40px -120px; }
.undo-icon							{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -155px; margin: 0px auto;}
.main-nav li:hover .undo-icon		{ background-position: -40px -155px; }
.redo-icon							{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -184px; margin: 0px auto;}
.main-nav li:hover .redo-icon		{ background-position: -40px -184px; }
.zoomin-icon						{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -215px; margin: 0px auto;}
.main-nav li:hover .zoomin-icon		{ background-position: -40px -215px; }
.zoomout-icon						{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -243px; margin: 0px auto;}
.main-nav li:hover .zoomout-icon		{ background-position: -40px -243px; }
.sizetool-icon						{ display: block; width: 40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -275px; margin: 0px auto;}
.main-nav li:hover .sizetool-icon	{ background-position: -40px -275px; }
.advSearch-icon						{ display: block; width:40px; height: 30px; background: url(../../images/accurate-image/nav-sprite.png) no-repeat 0 -307px; margin: 0px auto;}
.main-nav li:hover .advSearch-icon	{ background-position: -40px -307px; }
a.right-menu						{ background: #303030; color: #909090; height: 60px; width: 40px; display: block; float: right;  ;
									  font-size: 28px; text-align: center; line-height: 60px;}
a:hover.right-menu					{ text-decoration: none; color: #909090;}



/* iscroller styles
-------------------------------------------------- */

#wrapper 			{ position:absolute; z-index:1; left:0;	width:100%;	 overflow:auto;}
#scroller 			{ width:1125px;	height:100%; float:left; padding:0;}
#scroller ul 		{ list-style:none;display:block;float:left;	width:100%;	height:100%;padding:0;	margin:0;	text-align:left;}
#scroller li 		{ display:block;	vertical-align:middle;float:left;	padding:5px 20px;height:100%;border-left:1px solid #ccc; font-size:14px;}


/* popup overlay styles
-------------------------------------------------- */
.overlay-bg 		{ display: none; position: absolute; top: 0; left: 0; height:100%;width: 100%;cursor: pointer;z-index: 99999; background: #000; background: rgba(0,0,0,0.75);}
.overlay-content 	{ display: none; background: none; padding: 1%;/*	width: 96%;*/	position: relative;	top: 15%;  cursor: default; margin: 0 2%; border-radius: 4px; color: #fff;
					/*  box-shadow: 0 0 5px rgba(0,0,0,0.9);*/ position: relative;}
.inner-box			{ margin: 15px;}
.close-btn 			{ cursor: pointer; border: none; 	background: none; font-size: 42px; position: absolute;   left:50%;    margin-left:-20px; right: 5px; top: 5px;  }
.close-btn:hover 	{ background: none;}


.form-signin 			{max-width: 330px;padding: 15px;margin: 0 auto;}
#customWallForm input	{ margin-bottom: 10px; }



/* overide styles
-------------------------------------------------- */
.modal-dialog						{ width: 98%; margin: 2% 1%; color: #fff;}
.stickheader .dropdown-menu			{ left: inherit;right: 0px;border-radius: 0px;box-shadow: none;border: none;background: #303030; width: 220px; margin:0px; padding: 0px;}
.stickheader .dropdown-menu	li a 	{ background: none; text-decoration: none; color: #fff; font-family: 'HelveticaNeueLTCom67MediumCondensed'; font-size: 16px;
									  font-weight: normal; display: block;padding: 10px; }
 #cssmenu > ul 						{ margin:0px; padding: 0px; list-style-type: none; }
 #cssmenu ul li 					{ border-top: 1px solid #1f1f1f; border-bottom: 1px solid #4a4a4a; background: #303030;
									background: -moz-linear-gradient(top,  #303030 0%, #272727 100%);
									background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#303030), color-stop(100%,#272727));
									background: -webkit-linear-gradient(top,  #303030 0%,#272727 100%);
									background: -o-linear-gradient(top,  #303030 0%,#272727 100%);
									background: -ms-linear-gradient(top,  #303030 0%,#272727 100%);
									background: linear-gradient(to bottom,  #303030 0%,#272727 100%);
									filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#303030', endColorstr='#272727',GradientType=0 );}

.form-control 										{ height: 47px; padding: 12px 12px;}
.checkbox-margin .checkbox-inline+.checkbox-inline	{ margin-left: 0px;}
#login_btn											{ margin-bottom: 10px; }


/* Sub menu */

#cssmenu ul ul 								{ display: none; margin: 0px; padding: 0px; list-style-type: none; background: none;}
#cssmenu ul ul li 							{ padding: 5px; border: none; background: none; }
#cssmenu ul ul a 							{ display: block; padding: 3px  3px 3px 25px; }
#cssmenu ul ul a:hover 						{ color: #bcbcbc; text-decoration: none;}
 #cssmenu > ul > li.has-sub > a span 		{ background: url(../../images/accurate-image/icon_plus.png) 98% center no-repeat; display: block;}
#cssmenu > ul > li.has-sub.active > a span 	{ background: url(../../images/accurate-image/icon_minus.png) 98% center no-repeat; display: block;}
#cssmenu  a i 								{ font-size: 20px; color: #f0776e; text-shadow:1px 0 0 #000; margin-right: 4px;}




